<VBox fx:id="botones" prefHeight="576.0" prefWidth="208.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vista.controller.MenuAccionesAlgoformerController">
